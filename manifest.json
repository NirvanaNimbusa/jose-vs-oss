{
  "version": "0.57",
  "manifest_version": 2,
  "name": "José vs OSS",
  "description": "Help José to fight GitHub issues",

  "icons": {
    "16":  "icon-16.png",
    "48":  "icon-48.png",
    "128": "icon-128.png"
  },

  "content_scripts": [ {
    "js":      [ "jose.js" ],
    "matches": [ "https://github.com/*" ],
    "run_at":  "document_idle"
  } ]
}
